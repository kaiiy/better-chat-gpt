{
	"$schema": "https://biomejs.dev/schemas/1.3.1/schema.json",
	"organizeImports": {
		"enabled": true
	},
	
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,

			"a11y": {
				"useKeyWithClickEvents": "off",
				"useValidAnchor":"off",
				"useAnchorContent":"off"
			},
			"correctness": {
				"useExhaustiveDependencies": "off"
			},
			"style": {
				"noParameterAssign": "off"
			}
		}
	}
}
